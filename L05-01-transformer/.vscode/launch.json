  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Synth",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "pretrain",
        "synthesizer",
        "wiki.txt",
        "--writing_params_path",
        "synthesizer.pretrain.params"
      ]
    },
    {
      "name": "Python: charCorruption",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "charcorruption"
      ]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: L05",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "pretrain",
        "vanilla",
        "wiki.txt",
        "--writing_params_path",
        "vanilla.pretrain.params"
      ]
    }
  ]
}